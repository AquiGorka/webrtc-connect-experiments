{
  "name": "webrtc-connect-experiments",
  "version": "1.0.0",
  "main": "src/index.js",
  "author": "Gorka",
  "license": "MIT",
  "dependencies": {
    "google-url": "^0.0.4",
    "setimmediate": "^1.0.5",
    "simple-peer": "^8.1.1"
  },
  "scripts": {
    "env": "source .env",
    "start": "npm run env && budo src/index.js:js/bundle.js --dir src --live -- -t babelify -t envify",
    "setup": "rm -rf dist && mkdir -p dist/js",
    "assets": "cp src/index.html dist/index.html",
    "build": "npm run setup && npm run assets && browserify src/index.js -o dist/js/bundle.js -t babelify -t envify"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babelify": "^8.0.0",
    "browserify": "^14.5.0",
    "budo": "^10.0.4",
    "envify": "^4.1.0"
  }
}
